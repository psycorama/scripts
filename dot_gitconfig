[user]
	name = User_Name
	email = user_email
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[core]
	editor = emacs
	excludesfile = $HOME/.gitignore
[alias]
	st = status
	ba = branch -a
	logg = log --graph --branches --all --decorate=full
	logge = log --color --graph --all --pretty='format:%C(auto)%d%C(reset) %C(yellow)%h%C(reset) %ar %C(green)%aN%C(reset) %s'
	tableflip = "!sh -c \"rm -rf .*; rm -rf * ; cd .. ; git clone $(git config --get remote.origin.url)\""
[push]
	default = matching
